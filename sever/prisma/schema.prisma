datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Role {
  USER
  ADMIN
}

model User {
  id String @id @default(uuid())
  name  String
  email String  @unique
  password String
  avatar String?
  role Role @default(USER)
}